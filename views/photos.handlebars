<nav class="navbar navbar-inverse">
<a class="navbar-brand" href="#">
		{{#if isInstagram}}
        <img alt="Brand" src="/img/Instagram_logo.png" height="30" width="30">
		{{/if}}
		{{#if isFacebook}}
		<img src="/img/FacebookLogo.png" alt="Brand" height="30" width="30">
		{{/if}}
</a>
<a href="/logout" class="btn btn-default navbar-btn">Logout</a>

	{{#if isInstagram}}
    <p class="navbar-text">Hello, {{user.username}}</p>
	{{/if}}
	{{#if isFacebook}}
	<p class="navbar-text">Hello, {{user.displayName}}</p>
	{{/if}}
</nav>

{{#if photos}}
	<div class="row">
	  <div class="col-sm-6 col-md-4 col-lg-3">
	{{#each photos}}
	    <div class="thumbnail">
	      <img class="img-responsive img hashtag-img" src="{{url}}">
	      <div class="caption">
	      	<p>{{cap}}</p>
	      </div>
	    </div>
	{{/each}}
	  </div>
	</div>
{{else}}
	<div class="row">
		<div class="col-lg-12 text-center">
			<span>It seems you have no photos on your feed :(</span>
		</div>
	</div>
{{/if}}