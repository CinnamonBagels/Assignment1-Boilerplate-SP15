<nav class="navbar navbar-inverse">
<a class="navbar-brand" href="#">
		{{#if isInstagram}}
        <img alt="Brand" src="/img/Instagram_logo.png" height="30" width="30">
		{{/if}}
		{{#if isFacebook}}
		<img src="/img/FacebookLogo.png" alt="Brand" height="30" width="30">
		{{/if}}
</a>
<a href="/account" class="btn btn-default navbar-btn">Back to Account</a>
<a href="/logout" class="btn btn-default navbar-btn">Logout</a>

	{{#if isInstagram}}
    <p class="navbar-text">Hello, {{user.username}}</p>
	{{/if}}
	{{#if isFacebook}}
	<p class="navbar-text">Hello, {{user.displayName}}</p>
	{{/if}}
</nav>

{{#if likes}}
<div class="row">
	<div class="col-lg-6 col-lg-offset-3 text-center">
		<span><b>Here are some of your likes by name:</b></span>
	</div>
</div>
<div class="row">
	{{#each likes}}
	<div class="row">
		<div class="col-lg-6 col-lg-offset-3 text-center">
			<span>{{name}}</span>
		</div>
	</div>
	{{/each}}
</div>
<div class="row">
	<div class="col-lg-6 col-lg-offset-3 text-center">
		<span><b>Your most liked category is {{../mostLikes}} with {{../count}} likes</b></span>
	</div>
</div>
{{else}}
<div class="row">
	<div class="col-lg-6 col-lg-offset-3">
		<span><b>It seems you have no likes :(</b></span>
	</div>
</div>
{{/if}}
